def minion_game(string):
    # your code goes here
    score = 0
    words = ""
    stuart = {}
    kevin = {}
    vowel = [A,E,I,O,U]     
    for i in range(0,len(string)):
        score = 0
        words += string[i]
        score += 1
        stuart[words] = score  
    print(stuart)
    
if __name__ == '__main__':
    s = input()
    minion_game(s)
