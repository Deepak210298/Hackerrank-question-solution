def swap_case(s):
    new_s = ""
    for letter in s:
        if letter >= 'a' and letter <= 'z':
            new_s += letter.upper()
        elif letter >= 'A' and letter <= 'Z':
            new_s += letter.lower()
        else:
            new_s += letter
    return new_s

print(swap_case('HackerRank.com presents "Pythonist 2".'))
