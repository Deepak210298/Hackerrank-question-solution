n = int(input())
list = []
for i in range(0,n):
    word = str(input())
    list.append(word)
i = 0
cnt = 1
k = ""
for i in range(0,1):
    for j in range(0,n):
        if list[i] != list[j]:
            cnt += 1
        else:
            pass
print(cnt)            
        

cnt = 0
for i in range(0,n):
    if i == n-1:
        break
    cnt = 0
    for word in list:
        if list[i] == word:
            cnt += 1
    print(cnt, end=" ")
